{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "db:create": "node scripts/create-database.js",
    "db:destroy": "node scripts/drop-database.js",
    "db:init": "npm run db:createschema && npm run db:initdata",
    "db:createschema": "node scripts/execute-database.js resources/02-schema.sql",
    "db:initdata": "node scripts/execute-database.js resources/03-gyms.sql"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/websockets": "^4.5.8",
    "@types/sequelize": "^4.0.76",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "pgtools": "^0.3.0",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "sequelize": "^4.13.6",
    "typescript": "^2.6.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "nodemon": "^1.14.1",
    "tslint": "5.3.2",
    "ts-node": "^4.1.0"
  }
}
